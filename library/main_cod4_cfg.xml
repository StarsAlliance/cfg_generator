		<option name="sv_minrate" vartype="number" default="5000"></option>
		<option name="sv_maxRate" vartype="number" default="25000"></option>
		<option name="sv_minPing" vartype="number" default="0"></option>
		<option name="sv_maxping" vartype="number" default="300"></option>
